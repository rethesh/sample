{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://testyantra.shiftplanning.com/app/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@id='email']"
      },
      "text": "testyantra"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@id='password']"
      },
      "text": "testyantra123"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='LoginForm']//button[@name='login']"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//a[@id='sn_staff']"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//a[@id='s1']"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "(//input[@value='first name'])[1]"
      },
      "text": "Test"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "(//input[@value='last name'])[1]"
      },
      "text": "Yantric"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//button[@id='_as_save_multiple']"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//a[text()='All Staff']"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[contains(text(),'Test Yantric')]"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//td[b[text()='Delete:']]/following-sibling::td/a"
      }
    },
    {
      "type": "acceptAlert"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//a[@id='sn_dashboard']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//a[text()='Logout']"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}